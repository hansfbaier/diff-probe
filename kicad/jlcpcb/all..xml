    <Layers LengthUnit="mm">
      <Layer Color="#ff0000" FillMaterial="FR4_epoxy" Material="copper" Name="L2D0_Cu1" TargetLayer="Top" Thickness="0" Type="conductor"/>
      <Layer Color="#ffff00" FillMaterial="air" Material="copper" Name="L4D0_Cu2" TargetLayer="Bottom" Thickness="0" Type="conductor"/>
    </Layers>
  </Stackup>

  <ImportOptions AutoClose="true" ConvertClosedWideLinesToPolys="false">
    <Stitching/>
  </ImportOptions>

</c:Control>
